AR              = ar
MOD_ESMF        = /apps/software/standard/mpi/intel/18.0/intelmpi/18.0/esmf/7.1.0r-hdf5-1.8.22/mod/
LIB_ESMF        = /apps/software/standard/mpi/intel/18.0/intelmpi/18.0/esmf/7.1.0r-hdf5-1.8.22/lib/
INC_JASPER      = /apps/software/standard/compiler/intel/18.0/jasper/1.900.1/include/
