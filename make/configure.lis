INC_GRIBAPI     = /apps/software/standard/compiler/intel/18.0/grib_api/1.18.0/include/
LIB_GRIBAPI     = /apps/software/standard/compiler/intel/18.0/grib_api/1.18.0/lib/
INC_NETCDF      = /apps/software/standard/mpi/intel/18.0/intelmpi/18.0/netcdf/4.6.2-hdf5-1.8.22/include/
LIB_NETCDF      = /apps/software/standard/mpi/intel/18.0/intelmpi/18.0/netcdf/4.6.2-hdf5-1.8.22/lib/
INC_HDF4        = /apps/software/standard/compiler/intel/18.0/hdf/4.2.14-fortran/include/
LIB_HDF4        = /apps/software/standard/compiler/intel/18.0/hdf/4.2.14-fortran/lib/
INC_HDF5        = /apps/software/standard/mpi/intel/18.0/intelmpi/18.0/hdf5/1.8.22/include/
